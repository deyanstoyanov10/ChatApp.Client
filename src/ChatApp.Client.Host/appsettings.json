{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ClientConfiguration": {
    "KafkaConsumerConfig": {
      "ConsumerConfig": {
        "GroupId": "ChatAppClient",
        "BootstrapServers": "localhost:9092",
        "EnableAutoCommit": true,
        "AutoCommitInterval": 1000,
        "CompressionCodec": "gzip",
        "AutoOffsetReset": "earliest"
      },
      "MessageTopic": "messages-dev"
    }
  },
  "AllowedHosts": "*"
}
